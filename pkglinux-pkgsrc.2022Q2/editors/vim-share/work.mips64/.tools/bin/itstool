#!/bin/sh
tools_wrapper_sed=/2022Q2-mips64abi64/bin/nbsed
shquote()
{
	shquoted=$1
	case $shquoted in
	*\'*)
		shquoted=`$tools_wrapper_sed -e "s,','\\\\\\\\'',g" <<EOF
$shquoted
EOF`
	esac
	case $shquoted in
	(*[!!%+,\-./0-9:=@A-Z_a-z]*|'')
		case $shquoted in
		(*=*)	lhs=${shquoted%%=*}
			case $lhs in
			(*[!!%+,\-./0-9:=@A-Z_a-z]*|'') ;;
			(*)	shquoted="$lhs='${shquoted#*=}'"
				return
			esac
		esac
		shquoted="'$shquoted'"
	esac
}
shquote_args() {
	shquoted_args=''
	for arg in "$@"; do
		shquote "$arg"
		shquoted_args="$shquoted_args $shquoted"
	done
}
wrapperlog="${TOOLS_WRAPPER_LOG-/sda3/jails/current/pkglinux-pkgsrc.2022Q2/editors/vim-share/work.mips64/.work.log}"
shquote_args "$@"
echo "[*] "/sda3/jails/current/pkglinux-pkgsrc.2022Q2/editors/vim-share/work.mips64/.tools/bin/itstool"$shquoted_args" >> $wrapperlog
logprefix="set args$shquoted_args; shift; "
logmain=echo\ \>\>\ /sda3/jails/current/pkglinux-pkgsrc.2022Q2/editors/vim-share/work.mips64/.warning/itstool\ \"Please\ add\ USE_TOOLS+=itstool\ to\ the\ package\ Makefile.\"\;\ \ /bin/sh\ /sda3/jails/current/pkglinux-pkgsrc.2022Q2/mk/scripts/fail\ echo\ 1\>\&2\ \"ERROR:\"\ \"To\ use\ this\ tool,\ add\ USE_TOOLS+=itstool\ to\ the\ package\ Makefile.\"
echo '<.>' "$logprefix$logmain" >> $wrapperlog
echo >> /sda3/jails/current/pkglinux-pkgsrc.2022Q2/editors/vim-share/work.mips64/.warning/itstool "Please add USE_TOOLS+=itstool to the package Makefile.";  /bin/sh /sda3/jails/current/pkglinux-pkgsrc.2022Q2/mk/scripts/fail echo 1>&2 "ERROR:" "To use this tool, add USE_TOOLS+=itstool to the package Makefile."
