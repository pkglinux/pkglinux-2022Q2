# make agetty quiet
#quiet="yes"

# Set the baud rate of the terminal line
#baud=""
baud="115200"

# set the terminal type
term_type="linux"

# extra options to pass to agetty for this port
#agetty_options=""
#agetty_options="--autologin root --delay 1 --noclear tty1 38400"
agetty_options="--autologin alex --delay 1 --noclear tty1 38400"
